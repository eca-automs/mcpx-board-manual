{
  "name": "mcpx-board-manual-p",
  "version": "0.0.0-alpha-29",
  "description": "The manual of the MCP2015X-SMD and  PER16B-SMD boards with firmware P",
  "scripts": {
    "build": "node build.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "dev": "gitbook serve",
    "optimize:images": "jpegoptim -o  --size=15% src/images/*.jpg -d dist/; cp src/images/*.png dist && optipng dist/*.png",
    "watch:optimize:images": "watch 'npm run optimize:images' ./src/images",
    "preversion": "js-yaml .travis.yml",
    "version": "npm run optimize:images && npm run changelog && git add -A",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/eca-automs/mcpx-board-manual-p_it.git"
  },
  "author": "Eca",
  "license": "GFDL-1.3",
  "bugs": {
    "url": "https://gitlab.com/eca-automs/mcpx-board-manual-p_it/issues"
  },
  "homepage": "https://gitlab.com/eca-automs/mcpx-board-manual-p_it#README",
  "dependencies": {},
  "devDependencies": {
    "chalk": "^2.2.0",
    "conventional-changelog-cli": "^1.3.3",
    "js-yaml": "^3.10.0",
    "mkdirp": "^0.5.1",
    "watch": "^1.0.2"
  }
}
